{
    "dependencies": {
        "@sencha/ext-core": "5.1.0",
        "@somescope/somepackage": "1.0.0"
    },
    "sencha": {
        "name": "ext",
        "namespace": "Ext",
        "type": "framework",
        "creator": "Sencha",
        "summary": "Ext JS",
        "detailedDescription": "Sencha Ext JS JavaScript Framework",
        "version": "5.1.0",
        "compatVersion": "5.1.0",
        "format": "1",
        "output": "${package.dir}/build",
        "local": true,
        "sass": {
            "fashion": true,
            "namespace": "Ext",
            "etc": "${package.dir}/sass/etc/all.scss",
            "var": "${package.dir}/sass/var",
            "src": "${package.dir}/sass/src"
        },
        "classpath": "${package.dir}/src",
        "overrides": "${package.dir}/overrides",
        "resource": {
            "paths": [
                "resources"
            ]
        },
        "language": {
            "js": {
                "input": {
                    "version": "ES5"
                }
            }
        },
        "properties": {
            "skip.sass": 1,
            "skip.slice": 1
        },
        "extTier": "free"
    },
    "version": "5.1.0"
}